<FloatBuild>:
    MDCard:
        orientation:'vertical'
        pos_hint:{'center_x':.5,'center_y':.5}
        size_hint:None,None
        size:dp(300),dp(300)
        elevation:15
        Lb:
            text:'Rental'
            font_size:dp(40)
            adaptive_height:True
        MDSeparator:
            height:dp(2)
        MDGridLayout:
            cols:2
            MDBoxLayout:
                size_hint:.7,1
                canvas:
                    Color:
                        rgb:1,1,1
                    Ellipse:
                        size:100,100
                        pos:self.center_x-50,self.center_y-50
                        source:'pics/pic.jpg'
            MDBoxLayout:
                orientation:'vertical'
                Lb:
                    text:'Name of Building'
                Lb:
                    text:'Location'
                Lb:
                    text:'Rental Type'
                Lb:
                    text:'Rent Per Month'
                Lb:
                    text:'Rating'
                MDBoxLayout:
                    spacing:-12
                    orientation:'horizontal'
                    IconBtn:
                        id:star_one
                        icon:'star-outline'
                    IconBtn:
                        id:start_two
                        icon:'star-outline'
                    IconBtn:
                        id:star_three
                        icon:'star-outline'
                    IconBtn:
                        id:star_four
                        icon:'star-outline'
                    IconBtn:
                        id:star_five
                        icon:'star-outline'

        

<Lb@MDLabel>:
    font_name:'karantina'
<IconBtn@MDIconButton>:
    user_font_size:dp(20)